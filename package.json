{
  "type": "module",
  "name": "@mobx-ecosystem/mobx-react-query",
  "private": false,
  "version": "1.0.10",
  "description": "a wrapper that allows you to convenietly use react-query features with mobx",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "tsc --watch",
    "_clear": "rimraf dist",
    "tsc": "tsc",
    "_make-bundle": "rollup -c",
    "build": "run-s _clear _make-bundle",
    "pub": "yarn build && yarn publish --access public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "mobx-react-query",
    "react-query",
    "mobx-async",
    "mobx-query"
  ],
  "author": "RudikS",
  "license": "MIT",
  "peerDependencies": {
    "@tanstack/react-query": "^4",
    "mobx": "^4 || ^5 || ^6"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@tanstack/react-query": "^4.28.0",
    "mobx": "^6.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.0",
    "rollup": "^3.20.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RudikS-git/mobx-react-query"
  }
}
